{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "brace-style": "off",
    "keyword-spacing": "off",
    "comma-spacing": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "guard-for-in": "off",
    "max-len": "off",
    "no-undef": "off",
		"indent": ["error", 2]
  }
}
